
public class Sudoku {

	private int[][] array;
	
	public Sudoku(int[][] array){
		this.array = array;
	}
	
	public int[][] getArray() {
		return array;
	}

	public void setArray(int[][] array) {
		this.array = array;
	}

	public static void main(String[] args){
	    
		System.out.println(array.getArray());
		
	}
	
}
